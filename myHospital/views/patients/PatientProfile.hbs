<br />
<div class="row justify-content-md-center">
    <div class="col-md-4">
        {{#if message}}
            <div class="alert alert-primary" align="center">
                {{message}}
            </div>
        {{/if}}
	</div>
</div>

<div>
	<h1 align="center">Patient-profile</h1>
	<div class="row">


		<div class="col-md-4 mb-3">
			<h5>Name : {{profileValue.first_name}}</h5>
		</div>


	</div>

	<div class="row">
		<div class="col-md-4 mb-3">
			<h5>Patient Id: {{profileValue.patient_id}}</h5>
		</div>
	</div>
</div>
